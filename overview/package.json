{
    "name": "overview",
    "version": "0.0.1",
    "private": true,
    "description": "Flight Overview Report",
    "keywords": [
        "ui5",
        "openui5",
        "sapui5"
    ],
    "main": "webapp/index.html",
    "dependencies": {},
    "devDependencies": {
        "@ui5/cli": "^2.14.1",
        "@sap/ux-ui5-tooling": "1",
        "@sap/ux-specification": "UI5-1.100",
        "@sap/ux-ui5-fe-mockserver-middleware": "1",
        "rimraf": "3.0.2"
    },
    "scripts": {
        "start": "fiori run --open \"test/flpSandbox.html?sap-client=373&sap-ui-xx-viewCache=false#overview-tile\"",
        "start-local": "fiori run --config ./ui5-local.yaml --open \"test/flpSandbox.html?sap-client=373&sap-ui-xx-viewCache=false#overview-tile\"",
        "build": "ui5 build --config=ui5.yaml --clean-dest --dest dist",
        "deploy": "npm run build && fiori deploy --config ui5-deploy.yaml && rimraf archive.zip",
        "deploy-config": "fiori add deploy-config",
        "start-noflp": "fiori run --open \"index.html?sap-client=373&sap-ui-xx-viewCache=false\"",
        "start-mock": "fiori run --config ./ui5-mock.yaml --open \"test/flpSandbox.html?sap-client=373&sap-ui-xx-viewCache=false#overview-tile\"",
        "start-variants-management": "fiori run --open \"preview.html?&sap-client=373&sap-ui-xx-viewCache=false&fiori-tools-rta-mode=true&sap-ui-rta-skip-flex-validation=true#preview-app\"",
        "undeploy": "fiori undeploy --config ui5-deploy.yaml",
        "deploy-test": "npm run build && fiori deploy --config ui5-deploy.yaml --testMode true"
    },
    "ui5": {
        "dependencies": [
            "@sap/ux-ui5-tooling",
            "@sap/ux-ui5-fe-mockserver-middleware"
        ]
    },
    "sapux": true,
    "sapuxLayer": "CUSTOMER_BASE"
}
