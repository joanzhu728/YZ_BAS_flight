{
    "_version": "1.12.0",
    "sap.app": {
        "id": "cancel",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap-ux/fiori-elements-writer:worklist",
            "version": "0.1.9"
        },
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/sap/ZWKL_FLIGHTBOOKING_SRV/",
                "type": "OData",
                "settings": {
                    "annotations": [
                        "ZWKL_FLIGHTBOOKING_ANNO_MDL",
                        "annotation"
                    ],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            },
            "ZWKL_FLIGHTBOOKING_ANNO_MDL": {
                "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='ZWKL_FLIGHTBOOKING_ANNO_MDL',Version='0001')/$value/",
                "type": "ODataAnnotation",
                "settings": {
                    "localUri": "localService/ZWKL_FLIGHTBOOKING_ANNO_MDL.xml"
                }
            },
            "annotation": {
                "type": "ODataAnnotation",
                "uri": "annotations/annotation.xml",
                "settings": {
                    "localUri": "annotations/annotation.xml"
                }
            }
        },
        "crossNavigation": {
            "inbounds": {
                "cancel-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "cancel",
                    "action": "display",
                    "title": "{{flpTitle}}",
                    "subTitle": "{{flpSubtitle}}",
                    "icon": ""
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.100.2",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ushell": {},
                "sap.f": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.suite.ui.generic.template": {},
                "sap.collaboration": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "cancel.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false,
                    "metadataUrlParams": {
                        "sap-value-list": "none"
                    }
                }
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            }
        },
        "resources": {
            "css": []
        },
        "routing": {
            "config": {},
            "routes": [],
            "targets": {}
        }
    },
    "sap.ui.generic.app": {
        "_version": "1.3.0",
        "settings": {
            "forceGlobalRefresh": false,
            "objectPageHeaderType": "Dynamic",
            "considerAnalyticalParameters": true,
            "showDraftToggle": false
        },
        "pages": {
            "ListReport|ZWL_C_FILBOOK": {
                "entitySet": "ZWL_C_FILBOOK",
                "component": {
                    "name": "sap.suite.ui.generic.template.ListReport",
                    "list": true,
                    "settings": {
                        "smartVariantManagement": true,
                        "isWorklist": true,
                        "filterSettings": {
                            "dateSettings": {
                                "useDateRange": true
                            }
                        },
                        "_comment": "Comment: add quickVariantSelectionX from below",
                        "quickVariantSelectionX": {
                            "showCounts": true,
                            "variants": {
                                "1": {
                                    "key": "All",
                                    "entitySet": "ZWL_C_FILBOOK",
                                    "annotationPath": "com.sap.vocabularies.UI.v1.SelectionVariant#All",
                                    "tableSettings": { 
                                        "type": "GridTable",
                                        "multiSelect": true,
                                        "selectAll": true,
                                        "selectionLimit": 200 
                                    }
                                },
                                "2": {
                                    "key": "InProcess",
                                    "entitySet": "ZWL_C_FILBOOK",
                                    "annotationPath": "com.sap.vocabularies.UI.v1.SelectionVariant#InProcess",
                                    "tableSettings": { 
                                        "type": "GridTable",
                                        "multiSelect": true,
                                        "selectAll": true,
                                        "selectionLimit": 200 
                                    }
                                },
                                "3": {
                                    "key": "Approved",
                                    "entitySet": "ZWL_C_FILBOOK",
                                    "annotationPath": "com.sap.vocabularies.UI.v1.SelectionVariant#Approved",
                                    "tableSettings": { 
                                        "type": "GridTable",
                                        "multiSelect": true,
                                        "selectAll": true,
                                        "selectionLimit": 200 
                                    }
                                }
                            }
                        }
                },
                "pages": {}
            }
        }
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "transactional"
    }
}
}